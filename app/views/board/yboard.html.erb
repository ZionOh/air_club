						<div class="blogs">
						    <h3 class="tittle">YB 게시판</h3>
					<% @yblogs.each do |y| %>
                            <div class="blog-grid">
                                <% if y.image && y.url? %>
							    <%= link_to ysingle_path(y) do %><img src="/images/youtube.jpg" alt="" width="100%" height="250"/> <% end %>
                                <% elsif y.image? %>
							    <%= link_to ysingle_path(y) do %><img src="<%=y.image.url%>" alt="" width="100%" height="250"/> <% end %>
                                <% elsif y.url? %>
    						    <%= link_to ysingle_path(y) do %><img src="/images/youtube.jpg" width="100%" height="250" alt=""/><% end %>
                                <% else %>
							    <%= link_to ysingle_path(y) do %><img src="/images/air.jpg" width="100%" height="250" alt=""/> <% end %>
                                <% end %>
								 <div class="blog-info second">
									   <%= link_to profile_path(y.user.id) do %><p class="news"><%=y.user.username%></p><% end %>
									      <%= link_to ysingle_path(y) do %><p><%=y.content%></p><% end %>
											  <div class="bog_post_info infoPost">
												   <span class="datePost"><a href="#" class="post_date"><%=y.created_at.strftime("%d %b. %Y, %H:%M")%></a></span>
												    <div class="clearfix"></div>

												</div>
										  </div>
						     </div><br>
					<% end %>
						</div>
					        
