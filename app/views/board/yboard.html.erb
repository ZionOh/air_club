    <%= file_field_tag 'yimage' %><hr style="margin-top:-10px; visibility:hidden;" />
    <%= text_field_tag 'yvideo', nil, :placeholder => 'Copy the YOUTUBE URL here' %>
	<br>
						<input type="submit" value="submit" style="width:100px">
<% end %>
